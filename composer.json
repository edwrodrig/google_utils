{
    "name": "edwrodrig/google_utils",
    "description": "Library to retrieve information from google drive",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "google API utilities"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "require": {
      "php" : "^7.2",
      "google/apiclient": "^2.0",
      "ext-json": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\google_utils\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\google_utils\\" : "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            ".git",
            ".gitignore",
            "/.travis.yml",
            "/phpunit.xml.dist",
            "/examples",
            "/.idea",
            "README.md"
        ]
    }
}
